{
  "type": "footer",
  "name": "Footer",
  "sections": {
    "footer": {
      "type": "footer",
      "blocks": {
        "tagline-and-social-links": {
          "type": "tagline_and_social_links",
          "settings": {
            "tagline": "This is a demo store.",
            "show_social_icons": true
          }
        },
        "menus": {
          "type": "menus",
          "settings": {
            "show_menu_headings": false,
            "linklist_1": "main-menu",
            "linklist_2": "footer",
            "linklist_3": ""
          }
        },
        "newsletter": {
          "type": "newsletter",
          "settings": {
            "newsletter_heading": "Sign up to our newsletter"
          }
        }
      },
      "block_order": ["tagline-and-social-links", "menus", "newsletter"],
      "settings": {
        "show_payment_icons": true,
        "show_country_selector": true,
        "show_locale_selector": true,
        "color_scheme": "scheme2",
        "use_gradient_background": false
      }
    },
    "hero-logo": {
      "type": "hero-logo",
      "settings": {
        "home_page_only": false,
        "mobile_text_scale": 6,
        "desktop_text_scale": 10,
        "alignment": "center",
        "logo_max_width": 100,
        "disable_vertical_spacing": false,
        "enable_overlay": false,
        "overlay_offset": 100,
        "color_scheme": "scheme2",
        "use_gradient_background": false
      }
    }
  },
  "order": ["footer", "hero-logo"]
}
